import java.io.*;

public class PilhaIngenua {
	
	protected final static int MAX = 1000000;
	protected static Integer[] pilha;
	protected static int elementos;
	

	PilhaIngenua (){
		pilha = new Integer[MAX];
		elementos = 0;
	}
	
	static void add (int newElement){
		if (elementos == 0) {
			pilha[0] = newElement;
			elementos++;
		}
		else {
			for (int i = 0; pilha[i] != null; i++) {
				if (pilha[i+1] == null) {
					i++;
					pilha [i] = newElement;
					elementos++;
					break;
				}
			}
		}
	}

	static int remove() {
		int i;
		for (i = 0; pilha[i] != null; i++);
		int tmp = pilha[i - 1];
		pilha[i -1] = null;
		elementos--;
		return tmp;
	}
	
	public static void main(String[] args) {
		pilha = new Integer[MAX];
		try {
			BufferedReader entrada = new BufferedReader(new InputStreamReader(new FileInputStream("/home/tamires/Desktop/2020.2/AED/teste.txt")));
			while (entrada.ready()) {
				String linha = entrada.readLine();
				if (elementos == MAX -1) {
					System.out.println("Pilha cheia!");
					entrada.close();
				}
				else if(linha != null && !linha.isEmpty()) {
					int adicionar = Integer.parseInt(linha);
					add(adicionar);
				}
				else {
					System.out.println(remove());
			    	 
			    }
			}
			entrada.close(); 
		 }
		 catch (Exception e) {
		   System.out.println("Erro: " + e.getMessage());
		 }
	 }
}
